{
  "name": "trexim",
  "version": "2.0.0",
  "description": "Common components based on ant-design 4.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "antd",
    "ant",
    "trexim",
    "trex"
  ],
  "scripts": {
    "build": "father build",
    "lint:js": "eslint --cache --ext .ts --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/*\" --end-of-line auto",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Renderz/trexim"
  },
  "author": "renderz <464816158@qq.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@umijs/fabric": "^2.8.1",
    "babel-plugin-import": "^1.13.3",
    "father": "^2.30.10",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1"
  },
  "sideEffects": false,
  "lint-staged": {
    "src/*.ts": [
      "npm run lint:js",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
